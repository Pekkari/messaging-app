[Desktop Entry]
Type=Application
Name=tr("Messaging")
GenericName=tr("Messaging App")
Comment=tr("Messaging application")
Exec=messaging-app %u
Terminal=false
Icon=messaging-app
MimeType=x-scheme-handler/contact;x-scheme-handler/call
X-Ubuntu-Touch=true
X-Ubuntu-StageHint=SideStage
X-Ubuntu-Gettext-Domain=messaging-app
X-Ubuntu-Single-Instance=true
X-Screenshot=@CMAKE_INSTALL_PREFIX@/@MESSAGING_APP_DIR@/assets/messaging-app-screenshot.png
